{
    "name": "neeva-cli",
    "version": "1.0.0",
    "description": "Command-line tools for Neeva development",
    "main": "index.js",
    "private": true,
    "license": "ISC",
    "scripts": {
        "build": "tsc",
        "check-hash": "node ./check-hash.js",
        "check-deps": "node ./check-deps.js",
        "cli": "./bin/init-cli.js",
        "reset": "./reset.sh",
        "debug": "node inspect ./bin/init-cli.js",
        "clean": "rm -rf ./dist && rm -rf ./.src-hash",
        "format": "yarn run lint-check --fix && prettier --write '{src/**/*.js*,src/**/*.ts,src/**/*.tsx,src/**/*.graphql}'",
        "format-check": "prettier --check '{src/**/*.js*,src/**/*.ts,src/**/*.tsx,src/**/*.graphql}'",
        "jest": "node_modules/.bin/jest",
        "lint": "yarn lint-check && yarn format-check",
        "lint-check": "yarn eslint src --ext .js,.ts,.tsx",
        "test": "yarn lint && yarn type-check && yarn jest",
        "type-check": "yarn tsc --noEmit -p tsconfig.json"
    },
    "bin": {
        "init-cli": "bin/init-cli.js"
    },
    "devDependencies": {
        "@types/chokidar": "^2.1.3",
        "@types/ejs": "^3.0.4",
        "@types/folder-hash": "^3.3.0",
        "@types/jest": "^26.0.3",
        "@types/js-yaml": "^3.12.5",
        "@types/moment": "^2.13.0",
        "@types/node": "^12",
        "@types/node-fetch": "^2.5.12",
        "@types/shelljs": "^0.8.8",
        "@types/string-hash": "^1.1.1",
        "@types/terminal-kit": "^1.34.1",
        "@types/uuid": "^8.3.0",
        "@types/yargs": "^15.0.5",
        "@typescript-eslint/eslint-plugin": "^4.23.0",
        "@typescript-eslint/parser": "^4.23.0",
        "eslint": "^7.3.1",
        "eslint-plugin-sort-imports-es6-autofix": "^0.5.0",
        "folder-hash": "^3.3.3",
        "jest": "^26.1.0",
        "prettier": "^2.0.5",
        "ts-jest": "^26.1.1",
        "typescript": "^4.2.4"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.33.0",
        "@graphql-tools/merge": "^6.2.6",
        "@octokit/rest": "^18.0.1",
        "@types/diff": "^5.0.0",
        "@types/mkdirp": "^1.0.1",
        "@types/semver": "^7.3.1",
        "@types/webpack": "^4.41.22",
        "chalk": "^4.1.0",
        "chokidar": "^3.4.2",
        "chrome-webstore-upload": "^0.4.2",
        "diff": "^5.0.0",
        "ejs": "^3.1.7",
        "emittery": "^0.7.1",
        "format-graphql": "^1.4.0",
        "graphql": "^15.4.0",
        "graphql-tag": "^2.11.0",
        "js-yaml": "^3.14.0",
        "mkdirp": "^1.0.4",
        "moment": "^2.29.4",
        "node-fetch": "2.6.7",
        "semver": "^7.3.2",
        "shelljs": "^0.8.5",
        "simple-git": "^3.5.0",
        "string-hash": "^1.1.3",
        "terminal-kit": "^1.49.4",
        "tsconfig-paths": "^3.9.0",
        "uuid": "^8.3.1",
        "webpack": "^4.44.2",
        "yargs": "^15.4.0"
    }
}
